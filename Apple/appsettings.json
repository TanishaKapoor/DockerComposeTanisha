{
  "spring": {
    "application": {
      "name": "Apple"
    }
  },
  "eureka": {
    "client": {
      "shouldRegisterWithEureka": true,
      "serviceUrl": "http://eureka:8761/eureka",
      "ValidateCertificates": false
    },
    "instance": {
      "appName": "Apple",
      "hostName": "localhost",
      "port": "60662"
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "RabbitMQHostName": "rabbitmq"
}
