{
  "spring": {
    "application": {
      "name": "Banana"
    }
  },
  "eureka": {
    "client": {
      "shouldRegisterWithEureka": true,
      "serviceUrl": "http://eureka:8761/eureka/",
      "ValidateCertificates": false
    },
    "instance": {
      "appName": "Banana",
      "hostName": "localhost",
      "port": "52277"
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "RabbitMQHostName": "rabbitmq"
}
