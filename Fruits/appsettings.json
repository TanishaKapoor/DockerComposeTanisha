{
   "spring": {
    "application": {
      "name": "Fruits"
    }
  },
  "eureka": {
    "client": {
      "shouldRegisterWithEureka": true,
      "serviceUrl": "http://eureka:8761/eureka/",
      "ValidateCertificates": false
    },
    "instance": {
      "appName": "Fruits",
      "hostName": "localhost",
      "port": "58526"
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*"
}
